{
  "name": "javadocs-scraper",
  "version": "1.0.0",
  "description": "A TypeScript library to scrape JavaDocs information.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "github:Amgelo563/javadocs-scraper",
  "bugs": "https://github.com/Amgelo563/javadocs-scraper/issues",
  "homepage": "https://amgelo563.github.io/javadocs-scraper/",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "pnpm run clean && tsup ./src",
    "barrel": "pnpm dlx barrelsby -- -d ./src -D -q -H",
    "lint": "eslint src",
    "lint:file": "eslint --fix",
    "format": "prettier --write src",
    "format:file": "prettier --write",
    "prepare": "husky || true",
    "typedoc": "typedoc src/index.ts",
    "test": "vitest run"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "Amgelo563",
  "dependencies": {
    "@discordjs/collection": "2.1.1",
    "cheerio": "1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@commitlint/types": "19.5.0",
    "@eslint/js": "9.7.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/eslint__js": "8.42.3",
    "@types/node": "20.14.10",
    "@typescript-eslint/eslint-plugin": "8.6.0",
    "@typescript-eslint/parser": "8.6.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "domhandler": "5.0.3",
    "eslint": "9.7.0",
    "eslint-config-prettier": "9.1.0",
    "globals": "15.8.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "semantic-release": "24.2.0",
    "tsup": "8.3.5",
    "typedoc": "0.27.1",
    "typescript": "5.5.4",
    "typescript-eslint": "8.6.0",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.5.0",
  "lint-staged": {
    "*.ts": [
      "pnpm format:file",
      "pnpm lint:file"
    ]
  }
}
